pipeline {
    agent any
    environment {
      app = 'Davids app'  
    }

    stages {
        stage('"Project Info') {
            steps {
                echo 'Start Building $app'
            }
        }
        stage('Building stage') {
            steps {
                echo 'Building $app'
            }
        }
        stage('Build Result') {
            steps {
                echo '$app Build Succesfuly'
            }
        }
    }
}
