pipeline {
    agent any
    environment {
      app = 'Davids app'  
    }

    stages {
        stage('"Project Info') {
            steps {
                sh '''
                echo "Start Building $app"
                '''
            }
        }
        stage('Building stage') {
            steps {
                echo "Building $app"
            }
        }
        stage('Build Result') {
            steps {
                sh '''
                echo "$app Build Succesfuly"
                '''
            }
        }
    }
}
